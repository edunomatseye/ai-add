{
  "name": "ai-add",
  "version": "1.0.0",
  "description": "AI Add is a project that uses AI to add numbers.",
  "homepage": "https://github.com/edunomatseye/ai-add",
  "repository": {
    "type": "git",
    "url": "https://github.com/edunomatseye/ai-add.git"
  },
  "bugs": {
    "url": "https://github.com/edunomatseye/ai-add/issues"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test && npm run coverage",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "dev": "vitest"
  },
  "keywords": [
    "demo",
    "ai",
    "add",
    "numbers"
  ],
  "author": "Edunomatseye <edunomatseye@gmail.com> (https://github.com/edunomatseye)",
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
